<script>

    $('#color_palette li a').click(function(){
        let color = $(this).attr("ref");
        $(this).closest(".boxRow").find(".portHeadLightTitle span a").css("color",color);
    })

    $('.css-menu .ui-list li a').click(function(){
        var id = $(this).attr("id");
        $("div[id*='item_']").hide();
        $("div#"+id).show();
        var $msrContent = $('.tour_box_content');
		$msrContent.masonry({itemSelector: '.tour_box_masonry',percentPosition: true});
    });

    function kontrol(){
        
        var name = $('#name');
        var lname = $('#lastname');

        $('.ui-cfmodal__alert .required_list li').remove();

        if(name.css("display") != "none" && lname.css("display") != "none"){
            if(name.val() == "" && lname.val() == ""){
                $('.ui-cfmodal__alert .required_list').append('<li class="required"><i class="fa fa-terminal"></i>Eksik Veri : Ad</li>');
                $('.ui-cfmodal__alert .required_list').append('<li class="required"><i class="fa fa-terminal"></i>Eksik Veri : Soyad</li>');
            }
            else if (name.val() == ""){
                $('.ui-cfmodal__alert .required_list').append('<li class="required"><i class="fa fa-terminal"></i>Eksik Veri : Ad</li>');
            }
            else if(lname.val() == ""){
                $('.ui-cfmodal__alert .required_list').append('<li class="required"><i class="fa fa-terminal"></i>Eksik Veri : Soyad</li>');
            }
        }

        $('.ui-cfmodal__alert').fadeIn();

    }
    
    function font_inc(id, inc){
        var elem;
        if($(window).width() < 767){
            elem = $('table#mobil'+id);
        }
        else{
            elem = $('table#'+id);
        }
        var def = parseInt(elem.find('td').css('font-size'));
        
        if(inc == 0){
            def = 12;
        }
        else{
            if(inc > 0){
                def = def+inc;
            }
        
            if(inc < 0){
                def = def + (inc);
            }
        }
        

        elem.find('td').css('font-size',def);
        elem.find('th').css('font-size',def);
        elem.find('i').css('font-size',def);
        elem.find('span').css('font-size',def);
        //console.log(def);
    }

    /* Ajax List Gizle Göster Function */
    function check(e, table, indis){
        //console.log(indis);
        var table = $('table#'+$(table).attr("id"));
        if(e.classList.contains("active")){
            if($(window).width() < 767){
                table.find('tr').each(function(){
                    if($(this).attr("indis") == indis){
                        $(this).removeClass("hidden");
                    }
                });
            }
            else{
                table.find('tr').each(function(){
                    $(this).find('th').eq(indis).removeClass("hidden");
                    $(this).find('td').eq(indis).removeClass("hidden");
                });
            }
            
            e.classList.add("passive");
            e.classList.remove("active");
        }
        else{
            if($(window).width() < 767){
                table.find('tr').each(function(){
                    if($(this).attr("indis") == indis){
                        $(this).addClass("hidden");
                    }
                });
            }
            else{
                table.find('tr').each(function(){
                    $(this).find('th').eq(indis).addClass("hidden");
                    $(this).find('td').eq(indis).addClass("hidden");
                });
            }
            e.classList.add("active");
            e.classList.remove("passive");
        }
    }

    $(function(){ 

        var $msrContent = $('.tour_box_content');
		$msrContent.masonry({itemSelector: '.tour_box_masonry',percentPosition: true});

        $('.ui-cards-slide').slick({
            dots: true,
            infinite: true,
            arrows: false,
            speed: 500,
            autoplay: true,
            cssEase: 'linear'
        });

        $(".input-group-tooltip").mouseover(function() {
		    $( this ).closest("div.input-group").find( ".input-group_tooltip" ).stop().fadeIn("fast");
	    }).mouseout(function() {
		    $( this ).closest("div.input-group").find( ".input-group_tooltip" ).stop().fadeOut("fast");
	    });

       $('.ui-cfmodal-close').click(function(){
           $('.ui-cfmodal__alert').fadeOut();
       })

        $('.iconL > a').click(function(){
            var indis = $(this).parents('tr').index();
            $(this).toggleClass("iconL_open");
		    $(this).parents('tbody').find('tr').eq(indis+1).toggle();
	    })

        $('table[sort="true"]').dragtable({
            
            dragaccept: '.drag-enable',
            dragHandle: '.table-handle',
        })
        .tablesorter();

        /* Ajax List Gizle Göster*/
        $('#table_column_list').click(function(){
            var table = $(this).parents('.boxRow').find('table');
            if(table.css("display") != "none"){
                var table_id = table.attr("id");
                var table_list_container = $(this).parent().find('#table_list_container');
                if($(window).width() < 767){
                    var table_rep = table.find('tr:last-child').attr("indis");
                    if(!table_list_container.find('li').length){
                        for(var j=0; j<=table_rep; j++){
                            var data = table.find('tr').eq(j).find("td").eq(0).html();
                            var indis = table.find('tr').eq(j).attr("indis");
                            table_list_container.append('<li><input type="checkbox" onchange="check(this,'+ table_id +', '+ indis + ')">'+ data +'</li>');
                            table_list_container.find('.table-handle').remove();
                        }
                    }
                }
                
                else{
                    var table_th = table.find('thead th');
                    if(!table_list_container.find('li').length){
                        $.each(table_th, function(i){
                            var data = $(this).html();
                            var indis = $(this).index();
                            table_list_container.append('<li><input type="checkbox" onchange="check(this,'+ table_id +', '+ indis + ')">'+ data +'</li>');
                            table_list_container.find('li').eq(i).find('.table-handle').remove();
                        });
                    }
                }
            }
            
        });

        /* Bu sayfaya özeldir 
        $('.ui-dropdown > ul > li > a').parent().find("> ul").fadeIn();
         Bu sayfaya özeldir */

        var count = 0;
        $('.collapse').click(function(){
            $(this).parent().parent().find('.totalBoxBody').slideToggle();
            if($(this).find("span").hasClass("icon-minus")){
                $(this).find("span").removeClass("icon-minus").addClass("icon-pluss");
            }
            else{
                $(this).find("span").removeClass("icon-pluss").addClass("icon-minus");
            }
        });

        $('.multip-select').select2();
        $('.addRow').click(function(){
            var div = $('<div>');
            div.attr("class", "ui-card-item");
            
            var divBtn = $('<div>');
            divBtn.attr("class", "ui-card-item-hide");

            var divBtnLink = $('<a>');
            divBtnLink.attr({"class":"hideRow", "href":"javascript://"});

            divBtn.click(function(){
                $(this).parents('.ui-card-item').remove();
            })

            var divBtnLinkicon = '<i class="icon-minus" alt="Gizle"></i>';
            
            divBtnLink.append(divBtnLinkicon);
            divBtn.append(divBtnLink);
            div.append(divBtn);
            $(this).parent().parent().append(div);
        });
        $('.hideRow').click(function(){
            $(this).parents('.ui-card-item').remove();
        })
    })
</script>