<cfquery name="GET_ANALYSIS_QUESTIONS_RESULT" datasource="#dsn#">
	SELECT 
		RD.QUESTION_USER_ANSWERS,
		RD.RESULT_DETAIL_ID,
		RD.QUESTION_ID,
		RD.QUESTION_POINT,
		AR.RESULT_ID,
		AR.COMPANY_ID,
		AR.PARTNER_ID,
		AR.CONSUMER_ID
	FROM 
		MEMBER_ANALYSIS_RESULTS AR,
		MEMBER_ANALYSIS_RESULTS_DETAILS RD
	WHERE
		AR.RESULT_ID = RD.RESULT_ID AND
		AR.ANALYSIS_ID = #attributes.analysis_id# AND
		RD.QUESTION_ID = #GET_ANALYSIS_QUESTIONS.QUESTION_ID#
</cfquery>

