<cfquery name="GET_DETAILED_RESULT" datasource="#dsn#">
	SELECT 
		*
	FROM 
		MEMBER_ANALYSIS_RESULTS,
		MEMBER_ANALYSIS_RESULTS_DETAILS
	WHERE
		MEMBER_ANALYSIS_RESULTS.RESULT_ID = #attributes.RESULT_ID#
		AND
		MEMBER_ANALYSIS_RESULTS_DETAILS.RESULT_ID = MEMBER_ANALYSIS_RESULTS.RESULT_ID
</cfquery>
