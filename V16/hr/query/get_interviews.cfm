<cfquery name="GET_INTERVIEWS" datasource="#dsn#">
	SELECT
		EMPLOYEES_EVALUATION.EVAL_ID,
		EMPLOYEES_EVALUATION.PARS,
		EMPLOYEES_EVALUATION.POSS,
		EMPLOYEES_EVALUATION.RECORD_DATE,
		EMPLOYEES_APP.APP_POSITION,
		EMPLOYEES_APP.NAME,
		EMPLOYEES_APP.SURNAME,
		EMPLOYEES_APP.APP_NO
	FROM
		EMPLOYEES_APP,
		EMPLOYEES_EVALUATION
	WHERE
		EMPLOYEES_APP.EMPAPP_ID = EMPLOYEES_EVALUATION.EMPAPP_ID
</cfquery>
