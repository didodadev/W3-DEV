<cfquery name="GET_APP_PERF_RESULT" datasource="#dsn#">
	SELECT 
		PER_ID,
		PERFORM_POINT,
		USER_POINT
	FROM 
		EMPLOYEE_PERFORMANCE
	WHERE
		APP_EMP_ID = #attributes.EMPAPP_ID# AND
		RESULT_ID IN (SELECT RESULT_ID FROM EMPLOYEE_QUIZ_RESULTS WHERE QUIZ_ID = #QUIZ_ID# AND APP_EMP_ID = #attributes.EMPAPP_ID#)
</cfquery>
