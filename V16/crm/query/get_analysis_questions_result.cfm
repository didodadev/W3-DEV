<cfquery name="GET_ANALYSIS_QUESTIONS_RESULT" datasource="#dsn#">
	SELECT 
		*
	FROM 
		MEMBER_ANALYSIS_RESULTS,
		MEMBER_ANALYSIS_RESULTS_DETAILS
	WHERE
		MEMBER_ANALYSIS_RESULTS.RESULT_ID = MEMBER_ANALYSIS_RESULTS_DETAILS.RESULT_ID AND
		MEMBER_ANALYSIS_RESULTS.ANALYSIS_ID = #attributes.analysis_id# AND
		MEMBER_ANALYSIS_RESULTS_DETAILS.QUESTION_ID = #GET_ANALYSIS_QUESTIONS.QUESTION_ID#
</cfquery>

