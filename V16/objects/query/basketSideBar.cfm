<cfquery name="GET_BASKET_MAIN" datasource="#DSN3#">
    SELECT
        *
    FROM
        SETUP_BASKET
    WHERE
        B_TYPE = #b_type#
        AND BASKET_ID = #attributes.basket_id#
    ORDER BY
        BASKET_ID
</cfquery>
<cfquery name="GET_BASKET_INFO" datasource="#dsn3#">
	SELECT
		SBR.*,
        #DSN#.#dsn#.Get_Dynamic_Language(BASKET_ROW_ID,'#session.ep.language#','SETUP_BASKET_ROWS','TITLE_NAME',NULL,NULL,ISNULL(SLI.ITEM_#UCase(session.ep.language)#, TITLE_NAME)) AS ITEM
	FROM
		SETUP_BASKET_ROWS AS SBR
        LEFT JOIN #dsn_alias#.SETUP_LANGUAGE_TR AS SLI ON SBR.LANGUAGE_ID = SLI.DICTIONARY_ID 
    WHERE
		SBR.BASKET_ID = #attributes.basket_id# AND
		SBR.B_TYPE=#b_type#
	ORDER BY CASE
        <cfloop index="orderIndex" from="1" to="#listlen(basketInputList,',')-1#">
            WHEN SBR.LINE_ORDER_NO IS NOT NULL THEN SBR.LINE_ORDER_NO
        	WHEN SBR.TITLE = '#listgetat(basketInputList,orderIndex,",")#' THEN #orderIndex#
        </cfloop>
        ELSE
        	#listlen(basketInputList,',')#
        END
</cfquery>
