<script type="text/javascript">

    $(function() {   
        $( "#barkod_cihazi" ).after($('#basket_genel_bilgileri')).after($('#seperator'));
        $( ".dragabble" ).sortable();
    });

    function basketPageControl()
    {
        var mistake = false;
        var mistakeMessage = "";

        var el = $('.dragabble > .ui-sortable-handle');
        $.each(el, function(){
            $(this).find('#sira').val($(this).index());
        });

        if (document.add_basket_details.line_number.value.length != 0)
        { 

            $("input[name=module_content]:checked").each(function(){
                
                var minThirtyList = ["Tax","Kdv","list_price_discount","iskonto_tutar","disc_ount","disc_ount2_","disc_ount3_","disc_ount4_","disc_ount5_","disc_ount6_","disc_ount7_","disc_ount8_","disc_ount9_","disc_ount10_","price_net_doviz","other_money_value","other_money","price_other"];
                var minValue = 60;
                var minForKdviskonto = 35;
                var checkedValue = $(this).val();
                var numberName = checkedValue + "_genislik";
                var numberValue = $("input[name = "+numberName+"]").val();
            
                if(minThirtyList.indexOf(checkedValue) > -1 && numberValue < minForKdviskonto){
                    $("input[name = "+numberName+"]").css({"border-color":"red"});
                    mistake = true;
                    mistakeMessage = "<cfoutput>#getLang('KDV, iskonto ve döviz alanlarının genişliği minimum 35px olmalıdır','',48517)#</cfoutput>!";
                }else if(minThirtyList.indexOf(checkedValue) == -1 && numberValue < minValue){
                    $("input[name = "+numberName+"]").css({"border-color":"red"});
                    mistake = true;
                    mistakeMessage = "<cfoutput>#getLang('assetcare',458)#</Cfoutput>!";
                }else{
                    $("input[name = "+numberName+"]").css({"border-color":"#ccc"});
                }
            
            });
            
        }else{
            mistake = true;
            mistakeMessage = "<cf_get_lang dictionary_id='44424.Satır Sayısı Değer Girmelisiniz'>!";
        }	

        if(!mistake){
            AjaxFormSubmit('add_basket_details','addBasketDetailDiv',1,'<cf_get_lang dictionary_id="29723.Güncelleniyor.">','<cf_get_lang dictionary_id="29724.Güncellendi.">','','',1);
        }else{
            alert(mistakeMessage);
        }

        return false;

    }

    function changeTitle(inputName){
        $("input[name = '"+inputName+"']").val(1);
    }

</script>