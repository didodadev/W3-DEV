<cfquery name="CORRS" datasource="#dsn#">
	SELECT 
	    CORRCAT_ID, 
        CORRCAT, 
        DETAIL, 
        RECORD_DATE, 
        RECORD_EMP, 
        RECORD_IP, 
        UPDATE_DATE, 
        UPDATE_EMP, 
        UPDATE_IP 
    FROM 
    	SETUP_CORR
</cfquery>
<table>
    <cfif CORRS.recordcount>
		<cfoutput query="CORRS">
            <tr>
                <td width="20" align="right" valign="baseline" style="text-align:right;"><i class="fa fa-cube" style="font-size:12px;color:##FF9800;"></i></td>
                <td width="380"><a href="#request.self#?fuseaction=settings.form_upd_corr_cat&ID=#CORRCAT_ID#" class="tableyazi">#CORRCAT#</a></td>
            </tr>
        </cfoutput>
    <cfelse>
        <tr>
            <td width="20" align="right" valign="baseline" style="text-align:right;"><i class="fa fa-cube" style="font-size:12px;color:##FF9800;"></i></td>
            <td width="380"><cf_get_lang dictionary_id='57484.KayÄ±t Yok'>!</td>
        </tr>
    </cfif>
</table>
