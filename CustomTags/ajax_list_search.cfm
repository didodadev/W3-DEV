<cfset Randomize(round(rand()*1000000))/><cfparam name="attributes.id" default="search_box_#round(rand()*10000000)#"><cfparam name="attributes.title" default=""><cfparam name="attributes.search" default="1"><cfparam name="attributes.detailed_search" default="1"><cfset caller.last_table_id = attributes.id><cfoutput><cfif thisTag.executionMode eq "start">   <table class="ajax_list_search" id="<cfoutput>#attributes.id#</cfoutput>" align="center">	<tr>    	<td height="30">			<div class="ajaxlistSearchFormContent" style="width:100%;">				<div class="ajax_list_header" style="float:left;">#attributes.title#</div>				<div style="float:right; display:none; margin-top:3px;" id="#attributes.id#_open_area">				<a href="javascript://" onclick="#attributes.id#_islem_yap();">					<div id="#attributes.id#_image"><img src="images/find.gif"/></div>					<div id="#attributes.id#_image_close" style="display:none;"><img src="images/find_active.gif"/></div>				</a>				<script type="text/javascript">					function #attributes.id#_islem_yap()					{						gizle_goster(#attributes.id#_search_div);						gizle_goster(#attributes.id#_image);						gizle_goster(#attributes.id#_image_close);					}				</script>				</div> 	<cfelse>		</div>		</td>	</tr>    </table></cfif></cfoutput>